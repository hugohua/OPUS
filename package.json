  "scripts": {
    "dev": "next dev",
    "dev:all": "concurrently \"npm run dev:web\" \"npm run dev:worker\" \"npm run dev:tts\"",
    "dev:web": "next dev",
    "dev:worker": "npx tsx --watch workers/index.ts",
    "dev:tts": "npx tsx scripts/start-tts.ts",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "infra:up": "docker-compose up -d",
    "queue:enqueue": "npx tsx scripts/enqueue-drill.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:l0": "vitest run l0",
    "eval:l0": "npx tsx scripts/eval-prompts.ts --mode L0_SYNTAX --judge ets-auditor && npx tsx scripts/eval-prompts.ts --mode L0_PHRASE --judge ets-auditor && npx tsx scripts/eval-prompts.ts --mode L0_BLITZ --judge ets-auditor && npx tsx scripts/eval-prompts.ts --summary",
    "verify:l0": "npm run test:l0 && npm run eval:l0"
  },
